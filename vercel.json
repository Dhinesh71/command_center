{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/tracker/(.*)",
            "destination": "/backend/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "backend/server.js": {
            "maxDuration": 10
        }
    }
}